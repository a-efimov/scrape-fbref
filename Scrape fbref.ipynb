   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "import re\n",
    "import datetime as dt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "# парсим данные со страницы игры, всех полевых игроков, кроме вратарей\n",
    "\n",
    "def get_tables(url):\n",
    "    '''\n",
    "    url - адрес страницы, с которой парсим данные\n",
    "    '''\n",
    "    \n",
    "    # парсим страницу    \n",
    "    res = requests.get(url)\n",
    "    comm = re.compile(\"<!--|-->\")\n",
    "    soup = BeautifulSoup(comm.sub(\"\", res.text),'lxml')\n",
    "    \n",
    "    # забираем названия команд и дату игры\n",
    "    match_title = str(soup.title).split(' Match Report')[0].replace('<title>', '')\n",
    "    print(match_title, url)\n",
    "    matchweek = soup.find_all(text=re.compile('Matchweek'))[0].replace('(', '').replace(')', '').replace(' Matchweek ', '')\n",
    "    team_home = match_title.split(' vs. ')[0]\n",
    "    team_away = match_title.split(' vs. ')[1]\n",
    "    match_date = str(soup.find_all("span", {"class": "venuetime"})).split(" ")[2].split("=")[1].replace('"', '')\n",
    "    match_date = dt.datetime.strptime(str(soup.title).split(' Match Report')[1].split(' | ')[0].replace(' – ', ''), '%A %B %d, %Y')\n",
    "\n",
    "    # забираем статистику команд\n",
    "    all_tables = soup.findAll(\"tbody\")\n",
    "    \n",
    "    # статистика по хозяевам\n",
    "    home_summary = all_tables[0]\n",
    "    home_passing = all_tables[1]\n",
    "    home_pass_types = all_tables[2]\n",
    "    home_defensive = all_tables[3]\n",
    "    home_posession = all_tables[4]\n",
    "    home_other = all_tables[5]\n",
    "    \n",
    "    # статистика по гостям\n",
    "    away_summary = all_tables[7]\n",
    "    away_passing = all_tables[8]\n",
    "    away_pass_types = all_tables[9]\n",
    "    away_defensive = all_tables[10]\n",
    "    away_posession = all_tables[11]\n",
    "    away_other = all_tables[12]\n",
    "    \n",
    "    # сводим данные\n",
    "    summ_data = {\n",
    "        'url': url,\n",
    "        'match_date': match_date,\n",
    "        'match_title': match_title,\n",
    "        'matchweek': matchweek,\n",
    "        'team_home': team_home,\n",
    "        'team_away': team_away,\n",
    "        'stat_home': [home_summary, home_passing, home_pass_types, home_defensive, home_posession, home_other],\n",
    "        'stat_away': [away_summary, away_passing, away_pass_types, away_defensive, away_posession, away_other],\n",
    "    }\n",
    "    \n",
    "    return summ_data\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "# из каждой спарсенной таблицы формируем датафрейм pandas\n",
    "\n",
    "def get_frame(features, player_table):\n",
    "    '''\n",
    "    features - набор параметров статистики\n",
    "    player_table - спарсенные со страницы данные\n",
    "    '''\n",
    "    \n",
    "    pre_df_player = dict()\n",
    "    features_wanted_player = features\n",
    "    rows_player = player_table.find_all('tr')\n",
    "    players = []\n",
    "    for row in rows_player:\n",
    "        if(row.find('th',{\"scope\":\"row\"}) != None):\n",
    "            players.append(row.find('th',{\"scope\":\"row\"}).text.strip().encode().decode(\"utf-8\"))\n",
    "    \n",
    "            for f in features_wanted_player:\n",
    "                cell = row.find(\"td\",{\"data-stat\": f})\n",
    "                a = cell.text.strip().encode()\n",
    "                text=a.decode(\"utf-8\")\n",
    "                if(text == ''):\n",
    "                    text = '0'\n",
    "                if((f!='nationality')&(f!='position')&(f!='age')):\n",
    "                    text = float(text.replace(',',''))\n",
    "                if f in pre_df_player:\n",
    "                    pre_df_player[f].append(text)\n",
    "                else:\n",
    "                    pre_df_player[f] = [text]\n",
    "    df_player = pd.DataFrame.from_dict(pre_df_player)\n",
    "    df_player['player'] = players\n",
    "    \n",
    "    return df_player\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# полная работа функции\n",
    "\n",
    "def stat_match(season, features, url, final_columns):\n",
    "    '''\n",
    "    season - сезон матча\n",
    "    features - набор параметров статистики\n",
    "    url - адрес страницы, с которой парсим данные\n",
    "    final_columns - список столбцов получившейся таблицы\n",
    "    '''\n",
    "    \n",
    "    # парсим данные\n",
    "    data = get_tables(url)\n",
    "    \n",
    "    # из спарсенных данных формируем датафреймы\n",
    "    home_summary = get_frame(features['summary'], data['stat_home'][0])\n",
    "    home_passing = get_frame(features['passing'], data['stat_home'][1])\n",
    "    home_pass_types = get_frame(features['pass_types'], data['stat_home'][2])\n",
    "    home_defensive = get_frame(features['defensive'], data['stat_home'][3])\n",
    "    home_posession = get_frame(features['posession'], data['stat_home'][4])\n",
    "    home_other = get_frame(features['other'], data['stat_home'][5])\n",
    "    \n",
    "    away_summary = get_frame(features['summary'], data['stat_away'][0])\n",
    "    away_passing = get_frame(features['passing'], data['stat_away'][1])\n",
    "    away_pass_types = get_frame(features['pass_types'], data['stat_away'][2])\n",
    "    away_defensive = get_frame(features['defensive'], data['stat_away'][3])\n",
    "    away_posession = get_frame(features['posession'], data['stat_away'][4])\n",
    "    away_other = get_frame(features['other'], data['stat_away'][5])\n",
    "    \n",
    "    # объединаем все датафреймы в один\n",
    "    home_sum_data = home_summary.merge(home_passing, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\\\n",
    "    .merge(home_pass_types, how='left', on=['player', 'shirtnumber', 'nationality','position', 'age', 'minutes'])\\\n",
    "    .merge(home_defensive, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\\\n",
    "    .merge(home_posession, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\\\n",
    "    .merge(home_other, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\n",
    "    home_sum_data['team'] = data['team_home']\n",
    "    \n",
    "    away_sum_data = away_summary.merge(away_passing, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\\\n",
    "    .merge(away_pass_types, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\\\n",
    "    .merge(away_defensive, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\\\n",
    "    .merge(away_posession, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\\\n",
    "    .merge(away_other, how='left', on=['player', 'shirtnumber', 'nationality', 'position', 'age', 'minutes'])\n",
    "    away_sum_data['team'] = data['team_away']\n",
    "    \n",
    "    all_data = pd.concat([home_sum_data, away_sum_data], ignore_index=True)\n",
    "    all_data['season'] = season\n",
    "    all_data['team_home'] = data['team_home']\n",
    "    all_data['url'] = data['url']\n",
    "    all_data['match_title'] = data['match_title']\n",
    "    all_data['team_away'] = data['team_away']\n",
    "    all_data['match_date'] = data['match_date']\n",
    "    all_data['matchweek'] = data['matchweek']\n",
    "    \n",
    "    return all_data[final_columns]\n",
    "    "
   ]
  },
